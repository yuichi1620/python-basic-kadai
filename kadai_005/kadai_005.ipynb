        "# 上辺、下辺、高さの設定\n",
        "top = 10\n",
        "under = 20\n",
        "height = 5\n",
        "\n",
        "# 台形の面積計算\n",
        "area = (top + under) * height / 2\n",
        "\n",
        "# 結果の表示\n",
        "print(\"台形の面積は\", area, \"cm²です。\")"
