        "import random\n",
        "# 整数を代入\n",
        "var = random.randint(0, 15)\n",
        "\n",
        "# 生成した整数を表示\n",
        "print(var)\n",
        "# FizzBuzzの判定\n",
        "if var % 3 == 0 and var % 5 == 0:\n",
        "    print(\"FizzBuzz\")\n",
        "elif var % 3 == 0:\n",
        "    print(\"Fizz\")\n",
        "elif var % 5 == 0:\n",
        "    print(\"Buzz\")\n",
        "else:\n",
        "    print(var)"
      ]
    }
